<script>
// JavaScript function to open the chatbot popup
function openChatbotPopup() {
    var chatbotPopupUrl = "http://localhost:8080/chatbotapp/"; // URL of the chatbot JSP page
    var width = 700; // Width of the popup window
    var height = 450; // Height of the popup window
    var left = (window.innerWidth - width) / 2; // Calculate the left position to center the popup
    var top = (window.innerHeight - height) / 2; // Calculate the top position to center the popup
    var options = "width=" + width + ",height=" + height + ",left=" + left + ",top=" + top + ",resizable=yes"; // Popup window options
    window.open(chatbotPopupUrl, "ChatbotPopup", options); // Open the popup window
}
</script>



  

